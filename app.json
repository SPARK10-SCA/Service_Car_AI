{
  "expo": {
    "name": "sca_application",
    "slug": "sca_application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "favicon.png"
    }
  }
}
